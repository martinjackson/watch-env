{
  "name": "watch-env",
  "version": "0.10.2",
  "description": "a utility to convert .env file into ./src/initEnv.js that can be included in your front end code so the back end and front end can share project wide constants.",
  "main": "index.js",
  "bin": {
    "watch-env": "./index.js"
  },
  "scripts": {
    "test": "rm ./src/initEnv.js ; node index.js ",
    "watch": "rm ./src/initEnv.js ; node index.js --watch",
    "dry-run": "release-it --dry-run -n",
    "pub": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinjackson/watch-env.git"
  },
  "keywords": [
    "dotenv"
  ],
  "author": "Martin A. Jackson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martinjackson/watch-env/issues"
  },
  "homepage": "https://github.com/martinjackson/watch-env#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "parse-path": "^7.0.0",
    "process": "^0.11.10",
    "ps-list": "^8.1.0",
    "yargs": "github:yargs/yargs"
  },
  "devDependencies": {
    "release-it": "^15.5.0"
  }
}
